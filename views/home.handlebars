<div class="i-am-centered" style="margin: auto; max-width: max-content; margin-top: 50px; ">    
<div class="row">
{{#each data.people as |p|}}
    {{#> childEntry}}
    <div class="card" style="width: 18rem; padding-left: 10px; border-radius: 6px; margin-left: 10px;">
        <img src={{p.img}} style="width: 100px; margin-left: auto; margin-right: auto; background-color: rgb(19, 18, 18);" class="card-img-top" alt"No Image Found">
        <div class="card-body" style="background-color: rgb(19, 18, 18); border-radius: 6px; padding-top: 10px; margin-top: 10px;">
            <h5 class="card-title" style="color: aliceblue;">{{p.firstname}} {{p.lastname}}</h5>
            <p class="card-text" style="color: aliceblue;">Role : {{p.role}}</p>
            <a href="/details/{{p.id}}" class="btn btn-primary">Read More</a>
        </div>
    </div>
  {{/childEntry}}
{{/each}}
</div>
</div>
<h1>Meow Meow</h1>